{
  "name": "asterisk-callout",
  "version": "1.0.0",
  "description": "Asterisk callout app",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node --swc src/app.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migrations:generate": "npm run typeorm -- migration:generate -d ./src/data-source.ts",
    "migrations:create": "npm run typeorm -- migration:create -d ./src/data-source.ts",
    "migrations:run": "npm run typeorm migration:run -- -d ./src/data-source.ts",
    "migrations:revert": "npm run typeorm migration:revert -- -d ./src/data-source.ts"
  },
  "author": "Soslan Aldatov <soslanaldatov@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.4.1",
    "@types/ari-client": "^2.2.12",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "ari-client": "^2.2.0",
    "dotenv": "^16.4.4",
    "fastify": "^4.26.1",
    "pg": "^8.11.3",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0"
  }
}
